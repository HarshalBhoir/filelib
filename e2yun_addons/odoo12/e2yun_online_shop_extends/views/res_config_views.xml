<odoo>
    <data>
        <record id="online_shop_add_config_view" model="ir.ui.view">
            <field name="name">online.shop.add.config.view</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='webmaster_settings']" position="after">
                    <h2>在线商城</h2>
                    <div class="row mt16 o_settings_container" id="online_shop_settings">
                        <div class="col-12 col-lg-6 o_setting_box">

                            <div class="o_setting_right_pane">
                                <div class="row mt16">
                                    <label for="button_right_name"/>
                                    <field name="button_right_name"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>