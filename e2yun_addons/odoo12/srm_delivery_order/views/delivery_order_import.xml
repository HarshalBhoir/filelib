<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="delivery_order_import_form_action" model="ir.actions.act_window">
            <field name="name">Delivery Order Import</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">delivery.order.import</field>
            <field name="view_mode">tree</field>
        </record>


        <record id="delivery_order_import_tree_view" model="ir.ui.view">
            <field name="name">Delivery Order Import</field>
            <field name="model">delivery.order.import</field>
            <field name="arch" type="xml">
                <tree >
                    <field name="name"/>
                    <field name="create_date"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>